{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "LicenseEvaluation",
    "description": "Evaluation of the used licenses in the application",
    "type": "object",
    "properties": {
        "reasoning": {
            "type": "string",
            "description": "Reasoning that lead to the given result"
        },
        "summary": {
            "type": "string",
            "description": "Summary of used licenses"
        },
        "problems": {
            "type": "string",
            "description": "Description of any problems with the given licenses"
        },
        "licenses": {
            "type": "array",
            "description": "List of license",
            "items": {
                "type": "object",
                "title": "License",
                "description": "An individual License",
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "Name of the license"
                    }
                }
            }
        }
    }
}
